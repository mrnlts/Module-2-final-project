{{#each dbOrders}}
    <div class="user_orders_page_item">
        <img class="user_content_card_image" src="{{this.product.imageUrlProduct}}" class="offers_card_item_img" alt="{{title}}">
        <h4 class="content_card_title">{{this.product.description}}</h4>
        <p class="content_card_id">Price: {{this.product.price}}</p>
        <p class="content_card_user">Business: {{this.product.businessName.businessName}}</p>
        <p class="content_card_user">Order status: {{this.status}}</p>
    </div>
{{/each}}